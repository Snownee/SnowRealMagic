{
    "multipart": [
        {   "apply": { "model": "snowrealmagic:block/fence_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "snowrealmagic:block/fence_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "snowrealmagic:block/fence_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "snowrealmagic:block/fence_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "snowrealmagic:block/fence_side", "y": 270, "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "block/snow_height2" }
        }
    ]
}
